{
	"name": "carnet-de-bord",
	"version": "1.73.1",
	"engines": {
		"node": "16",
		"npm": "7"
	},
	"private": true,
	"repository": "git@github.com:SocialGouv/carnet-de-bord.git",
	"author": "Incubateur des Minist√®res Sociaux <dsi-incubateur@sg.social.gouv.fr> (https://incubateur.social.gouv.fr)",
	"bugs": "https://github.com/SocialGouv/carnet-de-bord/issues",
	"license": "Apache-2.0",
	"scripts": {
		"k8s": "yarn --silent --cwd .k8s",
		"hasura": "hasura",
		"hasura:console": "cd hasura && hasura console",
		"hasura:seed": "cd hasura && hasura seed apply",
		"hasura:migrate": "cd hasura && hasura migrate apply",
		"compose": "docker-compose up --build",
		"dev": "svelte-kit dev --host",
		"build": "svelte-kit build",
		"preview": "svelte-kit preview",
		"svelte-check": "svelte-check --tsconfig ./tsconfig.json",
		"svelte-check:watch": "svelte-check --tsconfig ./tsconfig.json --watch",
		"lint": "yarn lint:prettier && yarn lint:eslint && yarn svelte-check",
		"lint:prettier": "prettier --check --plugin-search-dir=. src",
		"lint:eslint": "eslint src .",
		"fix": "yarn fix:prettier && yarn fix:eslint",
		"fix:prettier": "prettier --write --plugin-search-dir=. src",
		"fix:eslint": "eslint src . --fix --cache",
		"format": "yarn fix:prettier",
		"codegen": "graphql-codegen --config codegen.cjs",
		"test": "jest",
		"test:watch": "jest --watch"
	},
	"devDependencies": {
		"@commitlint/cli": "^13.1.0",
		"@commitlint/config-conventional": "^13.1.0",
		"@graphql-codegen/cli": "2.1.1",
		"@graphql-codegen/typed-document-node": "^2.1.3",
		"@graphql-codegen/typescript": "2.2.0",
		"@graphql-codegen/typescript-operations": "2.1.3",
		"@graphql-codegen/urql-svelte-operations-store": "^1.1.3",
		"@sveltejs/kit": "next",
		"@tailwindcss/forms": "^0.3.3",
		"@tailwindcss/typography": "^0.4.1",
		"@testing-library/jest-dom": "^5.14.0",
		"@testing-library/svelte": "^3.0.0",
		"@types/cookie": "^0.4.0",
		"@types/jest": "^27.0.0",
		"@types/jsonwebtoken": "^8.5.5",
		"@types/nodemailer": "^6.4.4",
		"@types/rasha": "^1.2.3",
		"@types/tailwindcss": "^2.2.1",
		"@types/testing-library__jest-dom": "^5.14.0",
		"@types/uuid": "^8.3.3",
		"@typescript-eslint/eslint-plugin": "^4.31.0",
		"@typescript-eslint/parser": "^4.31.0",
		"autoprefixer": "^10.3.4",
		"babel-preset-vite": "^1.0.4",
		"cssnano": "^5.0.8",
		"esbuild": "^0.13.12",
		"esbuild-jest": "^0.5.0",
		"eslint": "^7.32.0",
		"eslint-config-prettier": "^8.3.0",
		"eslint-plugin-svelte3": "^3.2.0",
		"hasura-cli": "^2.0.8",
		"husky": "^7.0.2",
		"jest": "^27.0.0",
		"lint-staged": "^11.1.2",
		"postcss": "^8.3.6",
		"postcss-load-config": "^3.1.0",
		"prettier": "~2.3.2",
		"prettier-plugin-svelte": "^2.4.0",
		"remixicon": "^2.5.0",
		"svelte": "3.42.4",
		"svelte-check": "^2.2.5",
		"svelte-jester": "^2.0.1",
		"svelte-preprocess": "^4.9.1",
		"tailwindcss": "^2.2.10",
		"ts-jest": "^27.0.0",
		"tslib": "^2.3.1",
		"typescript": "^4.4.2"
	},
	"dependencies": {
		"@babel/core": "^7.16.0",
		"@babel/preset-env": "^7.16.0",
		"@babel/preset-typescript": "^7.16.0",
		"@gouvfr/dsfr": "^1.1.0",
		"@sveltejs/adapter-node": "^1.0.0-next.45",
		"@urql/core": "^2.3.1",
		"@urql/svelte": "^1.3.0",
		"cookie": "^0.4.1",
		"date-fns": "^2.23.0",
		"dotenv": "^10.0.0",
		"fast-deep-equal": "^3.1.3",
		"fnv-plus": "^1.3.1",
		"graphql": "^15.5.3",
		"jsonwebtoken": "^8.5.1",
		"jwt-decode": "^3.1.2",
		"nodemailer": "^6.6.3",
		"rasha": "^1.2.5",
		"svelecte": "^3.4.5",
		"svelte-accessible-dialog": "^2.1.3",
		"svelte-file-dropzone": "^0.0.15",
		"svelte-forms-lib": "^2.0.0",
		"uuid": "^8.3.2",
		"yup": "^0.32.11"
	},
	"type": "module",
	"lint-staged": {
		"*.{ts,js,cjs,mjs,svelte}": [
			"prettier --write --plugin-search-dir=.",
			"eslint --ignore-path .eslintignore --fix"
		],
		"*.{json,css,postcss,html,md}": [
			"prettier --write --plugin-search-dir=."
		]
	}
}
